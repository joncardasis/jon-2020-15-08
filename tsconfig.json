{
  "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "jsx": "react-native",
      "skipLibCheck": true,
      "lib": ["es2019"],
      "resolveJsonModule": true,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules", "babel.config.js", "metro.config.js", "jest.config.js"
  ]
}